{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"babel ./src/ --out-dir ./dist/ --watch --extensions .ts,.tsx\"",
    "build": "next build",
    "start": "next start",
    "m:create": "typeorm migration:create --name",
    "m:run": "typeorm migration:run",
    "m:revert": "typeorm migration:revert",
    "e:create": "typeorm entity:create --name",
    "seed": "node ./dist/seed.js"
  },
  "dependencies": {
    "next": "9.5.3",
    "next-images": "^1.6.2",
    "pg": "^8.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "concurrently": "^5.3.0",
    "file-loader": "^6.1.1"
  }
}
