{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "compile:src": "babel ./db/src/ --out-dir ./db/dist/ --extensions .ts,.tsx",
    "dev": "concurrently \"next dev\" \"yarn compile:src --watch\"",
    "build": "next build",
    "start": "next start",
    "m:create": "typeorm migration:create --name",
    "m:run": "typeorm migration:run",
    "m:revert": "typeorm migration:revert",
    "e:create": "typeorm entity:create --name",
    "seed": "node ./db/dist/seed.js"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "axios": "^0.21.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.3.2",
    "lodash": "^4.17.20",
    "marked": "^1.2.3",
    "md5": "^2.3.0",
    "next": "9.5.3",
    "next-images": "^1.6.2",
    "next-iron-session": "^4.1.9",
    "pg": "^8.4.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.2.1",
    "typeorm": "^0.2.28",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/highlight.js": "^10.1.0",
    "@types/lodash": "^4.14.164",
    "@types/marked": "^1.1.0",
    "@types/md5": "^2.2.1",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/styled-components": "^5.1.4",
    "concurrently": "^5.3.0"
  }
}
